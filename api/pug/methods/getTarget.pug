        &lt;ul id="list" class="list"&gt;
          &lt;li class="item" id="item-home"&gt;
            &lt;span&gt;Home&lt;/span&gt;
            &lt;a href="/sitemap#home" class="remove" data-id="home"&gt;删除&lt;/a&gt;
          &lt;/li&gt;
